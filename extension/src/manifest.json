{
    "manifest_version": 3,
    "name": "Webcam Authentication Extension",
    "description": "Capture webcam images and authenticate using DeepID.",
    "version": "1.0",
    "action": {
      "default_popup": "index.html", 
      "default_icon": "icon.png"
    },
    "permissions": [
      "storage", 
      "activeTab" 
    ],
    "background": {
      "service_worker": "background.js" 
    },
    "host_permissions": [
      "http://localhost:5000/*" 
    ],
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  